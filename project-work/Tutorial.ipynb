    "# I dataset sono forniti in .zip, il codice sottostante permette di unzippare i file \n",
    "# per ottenre i .csv contenuti.\n",
    "\n",
    "import zipfile\n",
    "import os\n",
    "\n",
    "def print_file_list():\n",
    "    for root, dirs, files in os.walk(\"dataset\"):\n",
    "       for name in files:\n",
    "          print(os.path.join(root, name))\n",
    "            \n",
    "def unzip(dataset):\n",
    "    zip_path = os.path.join(\"dataset\", dataset)\n",
    "    with zipfile.ZipFile(zip_path, 'r') as zip_ref:\n",
    "        zip_ref.extractall(\"dataset\")\n",
    "    \n",
    "print('unzipping man shoes')    \n",
    "unzip('man-shoes.zip')\n",
    "\n",
    "print_file_list()"
   ]
